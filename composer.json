{
    "name": "wboyz/phpxml",
    "description": "This is a package to serialize and deserialize php to xml.",
    "type": "library",
    "license": "MIT",
    "keywords": ["xml", "php", "serialize", "deserialize"],
    "require": {
        "php": ">=8.0",
        "ext-dom": "*",
        "crell/attributeutils": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.2",
        "squizlabs/php_codesniffer": "^3.10"
    },
    "scripts": {
        "lint": "phpcs --standard=phpcs.xml src/",
        "fix": "phpcbf --standard=phpcs.xml src/",
        "test": "phpunit tests"
    },
    "autoload": {
        "psr-4": {
            "Wboyz\\PhpXml\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wboyz\\PhpXml\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "eetir",
            "email": "bertalan.verebelyi@gmail.com"
        }
    ]
}
